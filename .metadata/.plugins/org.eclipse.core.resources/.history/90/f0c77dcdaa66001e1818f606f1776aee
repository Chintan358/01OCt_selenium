package fileio;

import java.io.File;

public class F003_File {
	public static void main(String[] args) {
		
		
		
		
		File file = new File("C:\\Chintan_work\\Files\\home.txt");
		
		
		
		if(file.exists())
		{
			System.out.println("file available");
		}
		else
		{
			System.out.println("file not availabe");
		}
		
		
	}
}
