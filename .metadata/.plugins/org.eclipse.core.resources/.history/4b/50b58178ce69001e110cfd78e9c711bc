package assignment;

public class A4 {
	public static void main(String[] args) {
		
		
		int a[] = {10,20,3,5,4,6,99,5};
		
		int max = a[0];
		int smax = a[0];
		for (int i = 0; i < a.length; i++) {
			if(a[i]>max)
			{
				smax = max;
				max = a[i];
			}
		}
		
		System.out.println("max : "+max);
		System.out.println("smax : "+smax);
	}
}
