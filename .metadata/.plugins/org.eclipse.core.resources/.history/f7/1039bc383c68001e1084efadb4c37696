package collection;

import java.util.ArrayList;

public class C001_ArraylistDemo {
	public static void main(String[] args) {
		
		//default size : 10
		//new size = oldsize * 3/2  + 1
		ArrayList<Integer> al = new ArrayList<Integer>();
		al.add(10);
		al.add(20);
		al.add(30);
		
		
		for(int i :al)
		{
			System.out.println(i);
		}
		
	}
}
