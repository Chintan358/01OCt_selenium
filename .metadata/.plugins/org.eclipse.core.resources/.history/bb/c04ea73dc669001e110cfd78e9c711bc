package collection;

import java.util.ArrayList;

public class C002_ArrayList {
	public static void main(String[] args) {
		
		
		
		ArrayList<String> sub = new ArrayList<String>();
		sub.add("java");
		sub.add("php");
		sub.add("android");
		
		
		for(String s : sub)
		{
			System.out.println(s);
		}
		
		
		
	}
}
